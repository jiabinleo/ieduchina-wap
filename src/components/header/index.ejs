        {"name":"留学","link":"//m.ieduchina.com/topic/abroad/"},
        {"name":"资源","link":"/#ziyuan"}]%>
        <ul>
            <%topmenu.forEach((el,index)=>{%>
                <li<%=(index==0)?" class=active":""%>>
                    <a href="<%=el.link%>"><%=el.name%></a>
                </li>    
            <%})%>
        </ul>
    </div>
    <div class="index-menu-wrap">
        <div class="index-menu">
            <div class="title">
                全部频道
                <div class="close-menu"></div>
            </div>
            <%let menu2=[
                {
                    "name":"资讯",
